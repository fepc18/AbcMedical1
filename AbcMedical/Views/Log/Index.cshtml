<div class="panel">
<header class="panel-heading">
    Bitácora (Log de la aplicación)
</header>
<div class="panel-body" style="padding-bottom:40px;">

    @model IEnumerable<Entities.Seguridad.Log>

    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_LayoutSecurity.cshtml";
    }




    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CompanyClient.RazonSocial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Modulo)
            </th>
           
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CompanyClient.RazonSocial)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Fecha)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Usuario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Modulo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Descripcion)
                </td>
               
            </tr>
        }

    </table>
</div>
</section>